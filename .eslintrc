{
	"parser": "babel-eslint",
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules": {
		"semi": "off",
		"linebreak-style": 0,
		"indent": [
			2,
			"tab"
		],
		"space-before-function-paren": "off",
		"no-tabs": 0,
		"operator-linebreak": "off",
		"comma-dangle": "off",
		"arrow-parens": "off",
		"object-curly-spacing": [
			2,
			"always"
		],
		"require-jsdoc": "off"
	},
	"extends": [
		"eslint:recommended",
		"google"
	]
}